.prodListImg img {
  margin: 0px;
  object-fit: cover;
  height: 50px;
  width: 50px;
  border-radius: 50%;
  max-width: none;
}
.orderForm td {
  padding: 5px 10px;
}
.filterBoxWrap {
  padding-bottom: 35px;
}
.prodListQty i {
  position: relative;
  top: 15px;
}
.prodListQty i:hover {
  cursor: pointer;
}
.prodListQty span i {
  font-size: 12px;
}
.orderSubmit {
  text-align: center !important;
}
.submitCheckout {
  background-color: #00c4b3;
  display: inline-block;
  padding: 15px 30px;
  width: auto;
  font-size: .75rem;
  line-height: 1.125rem;
  font-weight: 600;
  letter-spacing: .125rem;
  text-transform: uppercase;
  margin-left: 15px;
}
.submitCheckout:hover {
  background-color: #00deca;
}
@media screen and (max-width: 767px) {
  .orderForm td:first-child {
    position: static;
    opacity: 1;
  }
  
  .orderForm td {
    padding: 5px;
  }
}
@media screen and (max-width: 480px) {
  .prodListQty span {
    display: none;
  }
}
@media screen and (max-width: 414px) {
  .submitCheckout {
    margin: 0;
    padding: 10px;
  }
  
}
